    <input type="email"autocomplete="off" class="free-access-input" placeholder="Enter your email to get a free ticket" />
    <a href="" class="top-content-button" target="_blank">
        <button>
            {{ get_free_access_button }}
        </button>
    </a>
</div>

<div class="aibvs-nineth-container" style="padding-bottom:100px;padding-top: 100px;text-align: center;background:#151C2C;">
    <h1>{{ subscribe_label }}</h1>
    <div class="aibvs-nineth-content">
        <div class="aibvs-nineth-column" style="padding-right: 30px;">
            <input type="email" class="subscribe-input" autocomplete="off" placeholder="Enter your email to subscribe" />
        </div>
        <div class="aibvs-nineth-column">
            <a href="" class="top-content-button2" target="_blank">
                <button>
                    {{  subscribe_button_text }}
